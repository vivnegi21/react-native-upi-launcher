{
  "name": "react-native-upi-launcher",
  "version": "0.1.2",
  "description": "A React Native library to launch UPI intent in app and fetch installed UPI apps on Android.",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace react-native-upi-launcher-example",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "prepare": "bob build",
    "typecheck": "tsc"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vivnegi21/react-native-upi-launcher.git"
  },
  "author": "vivnegi21 <vivnegi21@gmail.com> (https://github.com/vivnegi21)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vivnegi21/react-native-upi-launcher/issues"
  },
  "homepage": "https://github.com/vivnegi21/react-native-upi-launcher#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@react-native-community/cli": "20.0.1",
    "@react-native/babel-preset": "0.81.1",
    "@types/react": "^19.1.0",
    "del-cli": "^6.0.0",
    "react": "19.1.0",
    "react-native": "0.81.1",
    "react-native-builder-bob": "^0.40.15",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@4.11.0",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "UpiLauncherSpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.upilauncher"
    }
  },
  "create-react-native-library": {
    "languages": "kotlin-objc",
    "type": "turbo-module",
    "tools": [],
    "version": "0.55.0"
  }
}
